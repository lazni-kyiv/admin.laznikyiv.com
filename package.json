{
  "name": "lazni-admin",
  "version": "1.0.3",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && zip -r dist.zip dist",
    "build:prod": "vite build --mode production",
    "build:stage": "vite build --mode staging",
    "deploy": "npm run build && firebase deploy",
    "deploy:prod": "npm run build:prod && firebase deploy --only hosting:prod",
    "deploy:stage": "npm run build:stage && firebase deploy --only hosting:stage",
    "serve": "firebase serve",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.19",
    "@fullcalendar/daygrid": "^6.1.19",
    "@fullcalendar/interaction": "^6.1.19",
    "@fullcalendar/multimonth": "^6.1.19",
    "@fullcalendar/timegrid": "^6.1.19",
    "@fullcalendar/vue3": "^6.1.19",
    "@iconify/vue": "^5.0.0",
    "@internationalized/date": "^3.8.2",
    "@vueuse/head": "^2.0.0",
    "appwrite": "^17.0.2",
    "chart.js": "^4.5.0",
    "dayjs": "^1.11.18",
    "echarts": "^6.0.0",
    "esbuild": "^0.25.8",
    "firebase": "^12.7.0",
    "lucide-vue-next": "^0.562.0",
    "pinia": "^3.0.1",
    "radix-vue": "^1.9.17",
    "signature_pad": "^5.1.3",
    "toastr": "^2.1.4",
    "uuid": "^11.1.0",
    "vue": "^3.5.13",
    "vue-chartjs": "^5.3.2",
    "vue-echarts": "^8.0.1",
    "vue-router": "^4.5.0",
    "ws": "^8.18.3"
